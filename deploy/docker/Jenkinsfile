pipeline {
    agent any

    stages {
        stage("Test Repo") {
            steps {
                echo "Testing the repo"
            }
        }
        stage("Test that all env vars have been gathered correctly") {
            steps {
                echo "Testing that all env vars have been gathered correctly"
                
                echo "Repository: ${params.Repository}"
                echo "Branch: ${params.Branch}"
                echo "Options: ${params.Options}"
                echo "Account: ${env.Account}"
                echo "Subdomain: ${env.Subdomain}"
            }
        }
    }
}
